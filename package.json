    "node": ">18.0.0",
    "pnpm": "^7.0.0"
  },
  "scripts": {
    "storybook": "npx nx run components:storybook",
    "story": "npx nx generate @nrwl/react:component-story --project=components",
    "lint": "npx nx run-many --target=lint --all --output-style=static",
    "lint-fix": "npm run lint -- --fix",
    "type-check": "npx nx run-many --target=type-check --all --output-style=static",
    "serve": "npx nx run-many --target=serve --all --output-style=stream",
    "serve:frontend": "npx nx run frontend:serve",
    "serve:backend": "npx nx run backend:serve",
    "test": "npx nx run-many --target=test --all --output-style=static",
    "test:coverage": "npx nx run-many --target=test --all --output-style=static -- --coverage",
    "migration:create": "cd libs/be-data/src/migration && npx dotenv -e ../../../../.env -- npx ts-node --project ../../tsconfig.json -r tsconfig-paths/register ../../../../node_modules/typeorm/cli.js migration:create --dataSource ./_dataSource.ts --pretty",
    "migration:generate": "cd libs/be-data/src/migration && npx dotenv -e ../../../../.env -- npx ts-node --project ../../tsconfig.json -r tsconfig-paths/register ../../../../node_modules/typeorm/cli.js migration:generate --dataSource ./_dataSource.ts --pretty",
    "migration:up": "npx dotenv -e ./.env -- npx ts-node --project libs/be-data/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --dataSource libs/be-data/src/migration/_dataSource.ts",
    "migration:down": "npx dotenv -e ./.env -- npx ts-node --project libs/be-data/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert --dataSource libs/be-data/src/migration/_dataSource.ts",
    "generate": "npx nx run-many --target=generate --all"
  },
  "private": true,
